<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/app_background"
    tools:context=".view.customer.OfficeMealDetailAsCustomerActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="216dp"
        android:layout_height="119dp"
        app:srcCompat="@drawable/background_nav"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="76dp" />

    <TextView
        android:id="@+id/txtProductName"
        android:layout_width="67dp"
        android:layout_height="23dp"
        android:text="Bún phở"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/txtProductPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="30,000 đ"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="212dp" />

    <TextView
        android:id="@+id/txtSupplierName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nhà hàng Hoàng Khoa"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Thanh Ba, Phú Thọ"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="46dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Đặt hàng"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="252dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Đánh giá"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="252dp" />
</androidx.constraintlayout.widget.ConstraintLayout>